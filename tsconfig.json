{
  "compilerOptions": {
    "sourceMap": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "strict": true,
    "lib": [
      "ES2022",
      "dom",
      "dom.iterable"
    ],
    "target": "ES2022",
    "declaration": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "jest.config.cjs"],
  "exclude": [
    "node_modules", "cjs", "esm"
  ]
}