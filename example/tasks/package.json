{
  "name": "bistrio",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/server",
    "prepare": "npm run bistrio:gen",
    "dev": "DEBUG=bistrio:*,restrant2:* tsc-watch --onSuccess 'node ./bin/server'",
    "prebuild": "npm run bistrio:gen",
    "build": "npx tsc && cpx 'views/**' dist/views && cpx 'public/**' dist/public",
    "clean": "npx rimraf dist",
    "console": "npx ts-node --project ./tsconfig.console.json -r ./server/support/console.ts",
    "link": "npm link ../restrant2-workspace/restrant2",
    "lint": "npm run eslint && npm run check-types",
    "lint:fix": "npm run eslint:fix && npm run check-types && npm run format",
    "eslint": "npx eslint ./server",
    "eslint:fix": "npx eslint ./server --fix",
    "format": "npx prettier --write 'server/**/*.{js,ts,tsx,json}'",
    "check-types": "npx tsc --noEmit",
    "db:seed": "npx ts-node prisma/seed.ts",
    "db:truncate": "npx ts-node prisma/truncate.ts",
    "build:client": "webpack",
    "bistrio:gen": "ts-node bin/generate.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "bistrio": "file:../../bistrio-0.2.0.tgz",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/http-errors": "^1.8.2",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.17",
    "@types/react-dom": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "cpx": "^1.5.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "8.4.0",
    "prettier": "^2.6.2",
    "prisma": "^3.9.2",
    "react-router-dom": "^6.3.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.0",
    "ts-morph": "^13.0.3",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^5.3.1"
  }
}
