{
  "name": "tasks",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/server",
    "dev:start": "NODE_ENV=production dotenv -e .env.development -- npm start",
    "prepare": "npm run bistrio:gen",
    "dev": "npm run watch",
    "dev:server": "NODE_ENV=development dotenv -e .env.development -- node -r ./server/support/tsnode_register ./bin/dev",
    "prewatch": "npm run bistrio:gen",
    "watch": "run-p watch:**",
    "watch:server": "npm-watch",
    "watch:livereload": "livereload -w 1500 -ee 'tsx,ts' 'dist/isomorphic,public'",
    "watch:client": "webpack --watch --config ./config/client/webpack.config.ts",
    "copy-assets": "cpx 'public/**' dist/public && cpx 'config/**' dist/config",
    "build": "npm run copy-assets && npm run bistrio:gen && npm run build:client && npm run build:server",
    "clean": "rimraf dist .cache .bistrio",
    "console": "NODE_ENV=development dotenv -e .env.development -- node  -r ./server/support/tsnode_register -r ./server/support/console.ts",
    "lint": "npm run eslint && npm run check-types",
    "lint:fix": "npm run eslint:fix && npm run check-types && npm run format",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "prettier --write './**/*.{js,ts,tsx,json,prisma}'",
    "check-types": "tsc --noEmit",
    "test": "PORT=4569 start-server-and-test test:server http://localhost:4569/tasks test:start",
    "pretest:server": "npm run db:truncate:test && npm run db:seed:test",
    "test:start": "NODE_ENV=test dotenv -e .env.test -- jest --runInBand .",
    "test:server": "NODE_ENV=test dotenv -e .env.test -- node ./bin/server",
    "db:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "db:reset": "dotenv -e .env.development -- prisma migrate reset",
    "db:rollback": "dotenv -e .env.development -- prisma migrate resolve --rolled-back",
    "db:seed": "dotenv -e .env.development -- prisma db seed",
    "db:truncate": "dotenv -e .env.development -- ts-node prisma/truncate.ts",
    "db:migrate:test": "NODE_ENV=test dotenv -e .env.test -- prisma migrate dev",
    "db:reset:test": "NODE_ENV=test dotenv -e .env.test -- prisma migrate reset --force",
    "db:seed:test": "NODE_ENV=test dotenv -e .env.test -- prisma db seed",
    "db:truncate:test": "NODE_ENV=test dotenv -e .env.test -- ts-node prisma/truncate.ts",
    "build:client": "webpack build --config ./config/client/webpack.config.ts",
    "build:server": "cpx .bistrio/filemap.json dist/.bistrio/ && tsc && tsc-alias",
    "build:isomorphic": "tsc --project ./config/tsconfig.isomorphic.json && tsc-alias --project ./config/tsconfig.isomorphic.json",
    "bistrio:gen": "node -r ./server/support/tsnode_register bin/generate.ts",
    "bistrio:gen:clean": "rimraf .bistrio"
  },
  "watch": {
    "bistrio:gen": {
      "patterns": [
        "isomorphic/routes",
        "server/endpoint"
      ],
      "runOnChangeOnly": true,
      "extensions": "ts,tsx"
    },
    "build:isomorphic": {
      "patterns": [
        ".bistrio",
        "isomorphic"
      ],
      "extensions": "ts,tsx"
    },
    "dev:server": {
      "patterns": [
        ".bistrio/filemap.json",
        "isomorphic",
        "server"
      ],
      "quiet": true,
      "inherit": true,
      "extensions": "ts,tsx"
    }
  },
  "prisma": {
    "seed": "node -r ./server/support/tsnode_register prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "bcrypt": "^5.1.0",
    "bistrio": "file:../../bistrio-0.5.2.tgz",
    "connect-redis": "^7.1.0",
    "cookie-parser": "~1.4.6",
    "express-session": "^1.17.3",
    "http-errors": "~2.0.0",
    "ioredis": "^5.3.2",
    "morgan": "~1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/expect-puppeteer": "^5.0.2",
    "@types/express-session": "^1.17.7",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.6",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/puppeteer": "^7.0.4",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cpx2": "^5.0.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.3.1",
    "jest-puppeteer": "^9.0.0",
    "livereload": "^0.9.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "prettier": "^3.0.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^4.9.0",
    "react-router-dom": "^6.7.0",
    "rimraf": "^5.0.1",
    "start-server-and-test": "^2.0.0",
    "ts-loader": "^9.4.2",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^5.1.3",
    "webpack": ">=5.76.0",
    "webpack-cli": "^5.1.1"
  },
  "overrides": {
    "word-wrap": "npm:@aashutoshrathi/word-wrap"
  }
}
