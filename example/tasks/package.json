{
  "name": "tasks",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./dist/server/boot.js",
    "dev:start": "NODE_ENV=production dotenv -e .env.development -- npm start",
    "prepare": "npm run bistrio:gen",
    "dev": "npm run watch",
    "predev:server": "npm run build:server",
    "dev:server": "cpx '.bistrio/filemap.json' dist/.bistrio && NODE_ENV=development dotenv -e .env.development -- node --inspect ./dist/server/boot.js",
    "prewatch": "npm run bistrio:gen",
    "watch": "npm run bistrio:gen && run-p dev:server watch:server",
    "watch:server": "NODE_ENV=development dotenv -e .env.development -- npm-watch",
    "copy-assets": "cpx 'public/**' dist/public",
    "build": "npm run bistrio:gen && npm run build:client && npm run build:server",
    "clean": "rimraf dist .cache .bistrio",
    "console": "DEBUG=-bistrio:console NODE_ENV=development dotenv -e .env.development -- node --import ./dist/server/console.js",
    "lint": "npm run eslint && npm run check-types",
    "lint:fix": "npm run eslint:fix && npm run check-types",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "prettier --check './**/*.{js,ts,tsx,json,prisma}'",
    "format:fix": "prettier --write './**/*.{js,ts,tsx,json,prisma}'",
    "check-types": "tsc --noEmit",
    "test": "PUPPETEER_DISABLE_HEADLESS_WARNING=true PORT=4569 start-server-and-test test:server http://localhost:4569/tasks test:start",
    "pretest:server": "npm run db:truncate:test && npm run db:seed:test",
    "test:start": "NODE_ENV=test dotenv -e .env.test -- jest --runInBand .",
    "test:server": "NODE_ENV=test dotenv -e .env.test -- node ./dist/server/boot.js",
    "db:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "db:reset": "dotenv -e .env.development -- prisma migrate reset",
    "db:rollback": "dotenv -e .env.development -- prisma migrate resolve --rolled-back",
    "db:seed": "dotenv -e .env.development -- prisma db seed",
    "db:truncate": "dotenv -e .env.development -- tsx prisma/truncate.ts",
    "db:migrate:test": "NODE_ENV=test dotenv -e .env.test -- prisma migrate dev",
    "db:reset:test": "NODE_ENV=test dotenv -e .env.test -- prisma migrate reset --force",
    "db:seed:test": "NODE_ENV=test dotenv -e .env.test -- prisma db seed",
    "db:truncate:test": "NODE_ENV=test dotenv -e .env.test -- tsx prisma/truncate.ts",
    "build:client": "tsx node_modules/.bin/webpack build --config ./config/client/webpack.config.ts",
    "build:server": "npm run copy-assets && cpx .bistrio/filemap.json dist/.bistrio/ && tsx node_modules/.bin/webpack build --config ./config/server/webpack.config.ts",
    "bistrio:gen": "tsx bin/generate.ts",
    "bistrio:gen:clean": "rimraf .bistrio"
  },
  "watch": {
    "bistrio:gen": {
      "patterns": [
        "universal/routes",
        "server/resources"
      ],
      "runOnChangeOnly": true,
      "extensions": "ts,tsx"
    },
    "build:server": {
      "patterns": [
        "universal",
        "server"
      ],
      "quiet": true,
      "inherit": true,
      "extensions": "ts,tsx"
    }
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "bcrypt": "^5.1.0",
    "bistrio": "file:../../bistrio-0.8.1.tgz",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "~1.4.6",
    "express-fileupload": "^1.4.3",
    "express-session": "^1.17.3",
    "http-errors": "~2.0.0",
    "ioredis": "^5.3.2",
    "morgan": "~1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "ts-jest": "^29.0.5",
    "tsx": "^4.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/expect-puppeteer": "^5.0.2",
    "@types/express-fileupload": "^1.4.4",
    "@types/express-session": "^1.17.7",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.6",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.10.0",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/react-dom": "^18.0.10",
    "@types/webpack-hot-middleware": "^2.25.9",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "babel-loader": "^9.1.3",
    "cpx2": "^5.0.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "express-list-routes": "^1.1.9",
    "jest": "^29.3.1",
    "jest-puppeteer": "^9.0.0",
    "livereload": "^0.9.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "prettier": "^3.0.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^4.9.0",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.7.0",
    "rimraf": "^5.0.1",
    "start-server-and-test": "^2.0.3",
    "ts-loader": "^9.4.2",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^5.3.3",
    "webpack": ">=5.76.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-middleware": "^7.0.0",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-node-externals": "^3.0.0"
  },
  "overrides": {
    "word-wrap": "npm:@aashutoshrathi/word-wrap"
  }
}
