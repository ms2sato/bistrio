{
  "compilerOptions": {
    "baseUrl": "../..",
    "paths": {
      "@/*": ["*"],
      "@bistrio/*": [".bistrio/*"]
    },
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "node16",
    "target": "es5",
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "moduleResolution": "node"
  },
  "include": ["../../.bistrio/**/*"]
}
